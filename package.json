{
  "name": "vue-kit",
  "private": true,
  "scripts": {
    "start": "cross-env APP_ENV=development webpack-dev-server --config config/webpack.client.config",
    "development": "cross-env APP_ENV=development webpack --config config/webpack.client.config",
    "staging": "cross-env APP_ENV=staging webpack --config config/webpack.client.config",
    "production": "cross-env APP_ENV=production webpack --config config/webpack.client.config",
    "dev:ssr:client": "cross-env APP_ENV=development SSR=1 webpack --config config/webpack.client.config",
    "dev:ssr:server": "cross-env APP_ENV=development SSR=1 webpack --config config/webpack.server.config",
    "staging:ssr:client": "cross-env APP_ENV=staging SSR=1 webpack --config config/webpack.client.config",
    "staging:ssr:server": "cross-env APP_ENV=staging SSR=1 webpack --config config/webpack.server.config",
    "production:ssr:client": "cross-env APP_ENV=production SSR=1 webpack --config config/webpack.client.config",
    "production:ssr:server": "cross-env APP_ENV=production SSR=1 webpack --config config/webpack.server.config",
    "lint": "stylelint --max-warnings 0 \"src/**/*.{vue,html,css,less,scss,sass}\" && eslint --max-warnings 0 \"src/**/*.{ts,js,vue}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,html,css,less,scss,sass}": "stylelint --max-warnings 0",
    "*.{ts,js,vue}": "eslint --max-warnings 0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.15.3",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "element-plus": "1.1.0-beta.7",
    "es6-promise": "^4.2.8",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "tslib": "^2.3.1",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@types/lodash": "^4.14.172",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/server-renderer": "^3.2.6",
    "autoprefixer": "^10.3.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-vue": "^7.17.0",
    "eslint-webpack-plugin": "^3.0.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "null-loader": "^4.0.1",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "sass": "^1.38.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "vue-loader": "^16.5.0",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
