{
  "name": "vue-kit",
  "private": true,
  "scripts": {
    "start": "yarn && husky install && cross-env APP_ENV=development webpack-dev-server",
    "dev": "yarn && cross-env APP_ENV=development webpack",
    "staging": "yarn && cross-env APP_ENV=staging webpack",
    "build": "yarn && cross-env APP_ENV=production webpack",
    "lint": "yarn && stylelint --max-warnings 0 \"src/**/*.{vue,html,css,less,scss,sass}\" && eslint --max-warnings 0 \"src/**/*.{ts,js,vue}\""
  },
  "lint-staged": {
    "*.{vue,html,css,less,scss,sass}": "stylelint --max-warnings 0",
    "*.{ts,js,vue}": "eslint --max-warnings 0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.9",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "element-plus": "1.0.2-beta.65",
    "es6-promise": "^4.2.8",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "tslib": "^2.3.0",
    "vue": "^3.1.5",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@types/lodash-es": "^4.17.4",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.1.5",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.7",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.15.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.1",
    "mini-css-extract-plugin": "^2.1.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "sass": "^1.37.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "vue-loader": "^16.4.0",
    "webpack": "^5.47.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-rc.0"
  }
}
