{
  "name": "vue-kit",
  "private": true,
  "scripts": {
    "start": "cross-env APP_ENV=development node ./config/dev.csr",
    "development": "cross-env APP_ENV=development webpack --config config/webpack.csr",
    "staging": "cross-env APP_ENV=staging webpack --config config/webpack.csr",
    "production": "cross-env APP_ENV=production webpack --config config/webpack.csr",
    "start:ssr": "cross-env APP_ENV=development node ./config/dev.ssr",
    "development:ssr:client": "cross-env APP_ENV=development webpack --config config/webpack.ssr.client",
    "development:ssr:server": "cross-env APP_ENV=development webpack --config config/webpack.ssr.server",
    "development:ssr": "yarn staging:ssr:client && yarn staging:ssr:server",
    "staging:ssr:client": "cross-env APP_ENV=staging webpack --config config/webpack.ssr.client",
    "staging:ssr:server": "cross-env APP_ENV=staging webpack --config config/webpack.ssr.server",
    "staging:ssr": "yarn staging:ssr:client && yarn staging:ssr:server",
    "production:ssr:client": "cross-env APP_ENV=production webpack --config config/webpack.ssr.client",
    "production:ssr:server": "cross-env APP_ENV=production webpack --config config/webpack.ssr.server",
    "production:ssr": "yarn production:ssr:client && yarn production:ssr:server",
    "lint": "stylelint --max-warnings 0 \"**/*.{vue,html,css,less,scss,sass}\" && eslint --max-warnings 0 \"**/*.{ts,js,vue}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,html,css,less,scss,sass}": "stylelint --max-warnings 0",
    "*.{ts,js,vue}": "eslint --max-warnings 0"
  },
  "engines": {
    "node": ">=14.17.5"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.15.4",
    "@vueuse/head": "^0.6.0",
    "axios": "^0.22.0",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "2.0.0-rc.12",
    "tslib": "^2.3.1",
    "vue": "^3.2.20",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@types/lodash": "^4.14.175",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/babel-plugin-jsx": "^1.1.0",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/server-renderer": "^3.2.20",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "cssnano": "^5.0.8",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-vue": "^7.19.1",
    "eslint-webpack-plugin": "^3.0.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.2",
    "less": "^4.1.2",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.2.2",
    "mini-css-extract-plugin": "^2.4.2",
    "null-loader": "^4.0.1",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "serialize-javascript": "^6.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "vue-loader": "^16.8.1",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.58.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
