{
  "name": "vue-kit",
  "private": true,
  "scripts": {
    "start": "yarn && husky install && cross-env APP_ENV=development BASE_URL=/ webpack-dev-server",
    "dev": "yarn && cross-env APP_ENV=development BASE_URL=/ webpack",
    "staging": "yarn && cross-env APP_ENV=staging BASE_URL=/ webpack",
    "build": "yarn && cross-env APP_ENV=production BASE_URL=/ webpack",
    "lint": "yarn && stylelint --max-warnings 0 \"src/**/*.{vue,html,css,less,scss,sass}\" && eslint --max-warnings 0 \"src/**/*.{js,vue}\""
  },
  "lint-staged": {
    "*.{vue,html,css,less,scss,sass}": "stylelint --max-warnings 0",
    "*.{js,vue}": "eslint --max-warnings 0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.10",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "element-plus": "1.0.2-beta.34",
    "es6-promise": "^4.2.8",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "vue": "^3.0.7",
    "vue-router": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@vue/compiler-sfc": "^3.0.7",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.7.0",
    "eslint-webpack-plugin": "^2.5.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "^16.1.2",
    "webpack": "^5.25.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.0.0-beta.0"
  }
}
