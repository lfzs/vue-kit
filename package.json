{
  "name": "vue-kit",
  "private": true,
  "scripts": {
    "start": "yarn && husky install && cross-env APP_ENV=development BASE_URL=/ webpack-dev-server",
    "dev": "yarn && cross-env APP_ENV=development BASE_URL=/ webpack",
    "staging": "yarn && cross-env APP_ENV=staging BASE_URL=/ webpack",
    "build": "yarn && cross-env APP_ENV=production BASE_URL=/ webpack",
    "lint": "yarn && stylelint --max-warnings 0 \"src/**/*.{vue,html,css,less,scss,sass}\" && eslint --max-warnings 0 \"src/**/*.{js,vue}\""
  },
  "lint-staged": {
    "*.{vue,html,css,less,scss,sass}": "stylelint --max-warnings 0",
    "*.{js,vue}": "eslint --max-warnings 0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.6",
    "axios": "^0.21.1",
    "dayjs": "^1.10.5",
    "element-plus": "1.0.2-beta.48",
    "es6-promise": "^4.2.8",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "vue": "^3.1.1",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.1.1",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.6",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.11.1",
    "eslint-webpack-plugin": "^2.5.4",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^2.2.0",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "^16.2.0",
    "webpack": "^5.39.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3"
  }
